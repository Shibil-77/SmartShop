        </div>
<div class="col-lg-9">
<div class="account-content">
<h4 class="account-content-title"><i class="far fa-map-marker-alt"></i> Products</h4>
<div class="account-content-table">
<div class="table-responsive">


<table class="table table-borderless">
    <tr>
    <th>Image</th>
    <th>Banner</th>
    <th>Action</th>
    </tr>
    <% banner_List.forEach(banner_List=> { %>
    <tr class="account-order-list">
    <td class="product-img"><img src="/img/banner/<%-banner_List.bannerimage %>" alt=""></td>
    <td><div class="product-info"><%-banner_List.Heading %></div></td>
    <td>
        <div class="product-info">
        <a href="/admin//deletebanner/<%=banner_List.id %>" class="content-action"  onclick="return confirm('Are you want delete')"><i class="far fa-trash"></i></a>
        <a href="/admin/editbanner/<%=banner_List.id %>" class="ms-2 content-action"><i class="far fa-pen"></i></a>
        </div>
        </td>
    </tr>
    <% }) %>
    </table>


</div>
<div class="col-12">

